@book{Schumpeter1942,
abstract = {Chapter 3 reviews the economic doctrine of Marx. Drawing his roots to Ricardo and Quesnay, Marx developed a theory of value as cornerstone to his theoretical structure, claiming that the value of a commodity is proportional to the amount of labour invested in it. The problem is that this theory does not work outside perfect competition, it considers labour the only input, and all labour of the same kind (marginal utility works better). Then there is the problem of capital goods, where Marx improved Ricardo's theory, introducing the distinction between constant and variable (=wage) capital, and showing that the return to capital comes out of exploitation. Wage is compensated for its real (=replacement, or renewal) value, but then the capitalist sets the number of working hours as superior to the number of hours needed to generate that wage, extracting surplus value. This surplus value is turned into constant capital, and also explains the difference between the labour value and the real price of commodities. This accumulation also tends to favour concentration, and the advent of big business. Note however that this theory is inapplicable, because it assumes that workers are produced according to rational costs calculations; competition and desire to expand production would also drive the surplus value to 0. However this theory could work in a dynamic sense, as equilibrium might never be achieved. The main critique Schumpeter moves is the lack of deeper analysis of these dynamics (his business cycles are mechanical and uninteresting), and in particular the lack of a theory of renewal and enterprise. Chapter 6 tries to explain the increase in wealth and production of the last half century. Shcumpeter starts by noting that capitalism is the only social system where the privileged class rise on the back of commercial success. Thus, the classics had an easy game at showing that profit maximisation and society improvement were compatible. Marshall and Wicksell completed this progress showing that not only are the two objectives compatible, they are basically one and the same, in perfect competition. The problem is, reality shows an awful lot of imperfect competition. Chapter 7 lays down the claim that the contradiction between theory and reality is due to the fact that the former deals with a static concept of equilibrium, while the latter is a dynamic process, constantly exposed to revolutions from within in a process of creative destruction. Chapter 8 elabor{\ldots}},
address = {New York},
author = {Schumpeter, Joseph A},
booktitle = {Economica},
chapter = {81-86},
number = {41},
pages = {40},
publisher = {Harper and Brothers},
title = {{Capitalism, Socialism and Democracy}},
volume = {11},
year = {1942}
}
