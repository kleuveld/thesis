doi = {10.1007/s10887-013-9093-1},
file = {:C$\backslash$:/Users/kld330/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rohner, Thoenig, Zilibotti - 2013 - Seeds of distrust Conflict in Uganda.pdf:pdf},
